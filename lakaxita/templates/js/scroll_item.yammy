{% verbatim %}
a 
    - href {{ url }}

    {{#if stretched_image}}
    img.slide
        - src {{ stretched_image }}
    {{else}}
    img.slide
        - src {{ default_image }}
    {{/if}}

    h3 
        | {{ title }}

    p.hover 
        | {{ hover }}

    {{#if icon}}
    img.icon
        - src {{ icon }}
    {{/if}}
{% endverbatim %}
